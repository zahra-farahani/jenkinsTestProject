pipeline {
  agent any
  stages {
    stage("build") {
      steps {
        echo 'building the application on branch dev...'
      }
    }
    stage("test") {
      steps {
        echo 'testing the application on branch dev...'
      }
    }
    stage("deploy") {
      steps {
        echo 'deploying the application on branch dev...'
      }
    }
  }
}
